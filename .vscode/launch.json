// launch.json
{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "xv6debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/kernel/kernel",
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "miDebuggerServerAddress": "localhost:26000", //见.gdbinit 中 target remote xxxx:xx
            "miDebuggerPath": "gdb-multiarch",
            "MIMode": "gdb",
            "preLaunchTask": "xv6build",
        }
    ]
}